{
    "version": 2,
    "builds": [
      {
        "command": "npm run build"
      },
      {
        "src": "api/server.js",
        "use": "@vercel/node",
        "config": {
          "memory": 1024,
          "includeFiles": "jobs.json"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/*",
        "dest": "api/server.js"
      }
    ]
  }